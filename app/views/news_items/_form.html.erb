<div class="col-xs-4"></div>
<div class="col-xs-4">

<%= form_for(@news_item) do |f| %>


  <div class="field">
    <%= f.label :titulo %><br>
    <%= f.text_field :titulo, class:"form-control" %>
  </div>
      <br>

      <div class="field">
    <%= f.label :link %><br>
    <%= f.text_field :link, class:"form-control" %>
  </div>

      <br>

      <div class="field">
    <%= f.label :image %><br>
    <%= f.file_field :image, class:"form-control" %>
  </div>
      <br>

      <div class="actions">
    <%= f.submit class:"btn btn-primary" %>
  </div>
<% end %>

</div>
<div class="col-xs-4"></div>
